<h1>New custom_event</h1>

<%= form_for(@custom_event) do |f| %>
  <%= f.error_messages %>



  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :pointValue %><br />
    <%= f.text_field :pointValue %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </p>
  <p>
    <%= f.label :wikip_url %><br />
    <%= f.text_field :wikip_url %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', custom_events_path %>